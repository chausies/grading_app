<h1>Courses</h1>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject</th>
      <th>School</th>
    </tr>
  </thead>
  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.subject %></td>
        <td><%= course.school %></td>
      </tr>
    <% end %>
  </tbody>
</table>
